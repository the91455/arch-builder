# API Keys - IMPORTANT: Fill these with your actual keys
GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Server Configuration
PORT=3000
NODE_ENV=development

# Model Configuration (Optional - uses sane defaults if not set)
# Use the most modern models available from each provider.
# For Gemini, provide a comma-separated list of models to try in order.
OPENAI_MODEL="gpt-4o"
ANTHROPIC_MODEL="claude-3-5-sonnet-20240620"
GEMINI_MODELS_FALLBACK_LIST="gemini-2.5-flash,gemini-2.5-pro,gemini-2.0-flash,gemini-2.0-flash-exp,gemini-1.5-flash,gemini-1.5-pro,gemini-pro"
